{
  "expo": {
    "name": "iot_app",
    "slug": "iot_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/gas.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/gas.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/gas.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thanh2002hcmx.iot_app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e78a5f0a-e7cd-442f-8ed0-b879259752f3"
      }
    }
  }
}
